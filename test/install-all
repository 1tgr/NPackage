#!/bin/sh
testDir=`dirname $0`
(
cd $testDir
rm -r bin/
mkdir -p bin
curl "http://build.partario.com/guestAuth/downloadArtifacts.html?buildTypeId=bt2&buildId=lastSuccessful" > bin/NPackage.zip
unzip bin/NPackage.zip -d bin/
rm -r lib/.dist lib/*
exec mono bin/NPackage.exe install --repository=../web/packages.js \
    fsharp.core \
    log4net \
    mono.cecil \
    mono.options \
    nhibernate \
    rhino.mocks \
    nunit \
    sharpziplib \

#    json \
#    quickgraph \

)
